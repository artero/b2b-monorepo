{
  "name": "nvs-b2b",
  "version": "2.0.0",
  "private": true,
  "main": "electron/dist/main.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4400 --open",
    "start:production": "ng serve --port 4400 --open --configuration=production",
    "start:local": "concurrently  \"yarn start:mock\" \"ng serve --port 4400 --open --configuration=local\"",
    "start:mock": "node mocks/mocks.js",
    "build": "ng build --configuration=production --base-href \"./\"",
    "build:electron": "yarn build && tsc -p electron && electron .",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "electron": "tsc -p electron && electron .",
    "electron:mac": "electron-builder --mac --c ./electron/electron-builder.json",
    "electron:windows": "electron-builder --windows --c ./electron/electron-builder.json",
    "electron:all": "yarn electron:mac && yarn electron:windows",
    "build:sketchSvgs": "/Applications/Sketch.app/Contents/Resources/sketchtool/bin/sketchtool export slices material/svgs.sketch --output=material/svgs",
    "build:svgpack": "svgpack material/svgs > ./src/assets/stylesheets/core/_svgs.scss",
    "build:svgs": "yarn build:sketchSvgs && yarn build:svgpack",
    "build:sketchIcons": "/Applications/Sketch.app/Contents/Resources/sketchtool/bin/sketchtool export slices material/icons.sketch --output=material/icons",
    "build:iconFontGenerator": "icon-font-generator material/icons/*.svg -c false --html false -o ./src/assets/fonts --types 'woff,woff2' -n 'nvs-icons'",
    "build:icons": "yarn build:sketchIcons && yarn build:iconFontGenerator"
  },
  "dependencies": {
    "@angular/animations": "^17.0.5",
    "@angular/cdk": "^17",
    "@angular/common": "^17.0.5",
    "@angular/compiler": "^17.0.5",
    "@angular/core": "^17.0.5",
    "@angular/forms": "^17.0.5",
    "@angular/localize": "^17.0.5",
    "@angular/material": "^17",
    "@angular/platform-browser": "^17.0.5",
    "@angular/platform-browser-dynamic": "^17.0.5",
    "@angular/router": "^17.0.5",
    "@angular/ssr": "^17.0.5",
    "@ngx-translate/core": "^15.0.0",
    "@ngx-translate/http-loader": "^8.0.0",
    "@types/jspdf": "^1.3.3",
    "bootstrap": "^4.6.1",
    "concurrently": "^6.2.0",
    "http-parser-js": "^0.5.3",
    "jspdf": "2.3.1",
    "html2canvas": "1.0.0-rc.7",
    "lodash": "^4.17.21",
    "ngx-cookie-service": "^17.0.0",
    "rxjs": "~6.5.5",
    "tslib": "^2.0.0",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.5",
    "@angular/cli": "^17.0.5",
    "@angular/compiler-cli": "^17.0.5",
    "@angular/language-service": "^17.0.5",
    "@marsbased/marstyle-angular": "^1.5.1",
    "@marsbased/marstyle-stylelint": "^3.0.0",
    "@marsbased/svgpack": "^1.0.3",
    "@types/electron": "^1.6.10",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "electron": "^11.0.1",
    "icon-font-generator": "^2.1.11",
    "jasmine-core": "~5",
    "jasmine-spec-reporter": "~7",
    "json-server": "~0.17.0",
    "karma": "~6.3.9",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "prettier": "^2.4.1",
    "protractor": "~7.0.0",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "typescript": "~5.2.2"
  }
}
